<?xml version="1.0" encoding="UTF-8"?>
<!--这个配置文件也是使用myspring框架的开发人员提供的。-->
<beans>

    <bean id="user" class="com.byonecup.myspring.bean.User">
        <property name="name" value="张三"></property>
        <property name="age" value="18"></property>
    </bean>

    <bean id="userDaoBean" class="com.byonecup.myspring.bean.UserDao"></bean>

    <bean id="userService" class="com.byonecup.myspring.bean.UserService">
        <property name="userDao" ref="userDaoBean"/>
    </bean>

</beans>